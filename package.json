{
  "name": "openapi-autoclient-csharp",
  "version": "1.0.0",
  "description": "Automatic generator of C# clients from OpenAPI specifications.",
  "main": "lib/main.js",
  "directories": {
    "test": "test"
  },
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "format": "prettier --write 'src/**/*.ts'",
    "lint": "eslint --fix 'src/**/*.ts'",
    "test": "npm run build && jest --config jest.config.ts",
    "test:coverage": "jest --config jest.config.ts --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlexandreBellas/openapi-autoclient-csharp.git"
  },
  "keywords": [
    "openapi",
    "swagger",
    "csharp",
    "javascript",
    "typescript",
    "client",
    "automatic",
    "code",
    "specification"
  ],
  "author": "Alexandre Batistella Bellas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AlexandreBellas/openapi-autoclient-csharp/issues"
  },
  "homepage": "https://github.com/AlexandreBellas/openapi-autoclient-csharp#readme",
  "dependencies": {},
  "devDependencies": {
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "jest": "^29.7.0"
  },
  "publishConfig": {
    "access": "public"
  }
}